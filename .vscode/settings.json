{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "search.useIgnoreFiles": true,
  "cSpell.words": [
    "antipatterns",
    "aura",
    "bazel",
    "Contentful",
    "FORMLY",
    "lintstagedrc",
    "primeng",
    "primeui",
    "tailwindcss",
    "WCAG"
  ]
}
